UPDATE pilothouse.wwwwwwwasabi_infora02_reporting AS r
SET
    "Status" = COALESCE(b."Status", r."Status")
FROM pilothouse.wwwwwwwasabi_infora02_base AS b
WHERE
    r."pam_timestamp" = b."pam_timestamp"
    AND r."APPLICATION_BAM_VALUE" = b."APPLICATION_BAM_VALUE"
    AND r."APPLICATION" = b."APPLICATION"
    AND r."SERVER_ID" = b."SERVER_ID"
    AND r."ENVIRONMENT_Server" = b."ENVIRONMENT_Server"
    AND r."AREA" = b."AREA"
    AND r."INFRAVERSION" = b."INFRAVERSION"
    AND r."PAMELA__COUNTRY" = b."PAMELA__COUNTRY"
    AND r."PAM_TECHNOTYPE" = b."PAM_TECHNOTYPE"
    AND r."PAM_CLUSTERNAME" = b."PAM_CLUSTERNAME"
    AND r."metier" = b."metier"
    AND r."PAM_LOGINNAME" = b."PAM_LOGINNAME"
    AND r."Account_Type" = b."Account_Type"
    AND r."productionmanager" = b."productionmanager"
    AND r."PAM_LOGINLASTCONNECTION" = b."PAM_LOGINLASTCONNECTION"
    AND r."dapsproductionManagerDelegatesname" = b."ManagerDelegatesname"
    AND r."appStatus" = b."appStatus"
    AND r."UID_User" = b."UID_User"
    AND r."Assignee" = b."Assignee"
    AND r."Action" = b."Action"
    AND r."Remediation" = b."Remediation"
    AND r."Comments" = b."Comments"
    AND r."ImportDate" = (
        SELECT MAX("ImportDate")
        FROM pilothouse.wwwwwwwasabi_infora02_reporting AS r2
        WHERE
            r2."pam_timestamp" = b."pam_timestamp"
            AND r2."APPLICATION_BAM_VALUE" = b."APPLICATION_BAM_VALUE"
            AND r2."APPLICATION" = b."APPLICATION"
            AND r2."SERVER_ID" = b."SERVER_ID"
            AND r2."ENVIRONMENT_Server" = b."ENVIRONMENT_Server"
            AND r2."AREA" = b."AREA"
            AND r2."INFRAVERSION" = b."INFRAVERSION"
            AND r2."PAMELA__COUNTRY" = b."PAMELA__COUNTRY"
            AND r2."PAM_TECHNOTYPE" = b."PAM_TECHNOTYPE"
            AND r2."PAM_CLUSTERNAME" = b."PAM_CLUSTERNAME"
            AND r2."metier" = b."metier"
            AND r2."PAM_LOGINNAME" = b."PAM_LOGINNAME"
            AND r2."Account_Type" = b."Account_Type"
            AND r2."productionmanager" = b."productionmanager"
            AND r2."PAM_LOGINLASTCONNECTION" = b."PAM_LOGINLASTCONNECTION"
            AND r2."dapsproductionManagerDelegatesname" = b."ManagerDelegatesname"
            AND r2."appStatus" = b."appStatus"
            AND r2."UID_User" = b."UID_User"
            AND r2."Assignee" = b."Assignee"
            AND r2."Action" = b."Action"
            AND r2."Remediation" = b."Remediation"
            AND r2."Comments" = b."Comments"
    );
